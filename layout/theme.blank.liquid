<!doctype html>
<!--[if lt IE 7]><html class="no-js ie6 oldie" lang="en"><![endif]-->
<!--[if IE 7]><html class="no-js ie7 oldie" lang="en"><![endif]-->
<!--[if IE 8]><html class="no-js ie8 oldie" lang="en"><![endif]-->
<!--[if gt IE 8]><!--><html lang="en"><!--<![endif]-->
<head>
  <!-- v1.4.3 -->
  <meta charset="utf-8">
  <!--[if IE]>
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<![endif]-->
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta name="keywords" content="{{ "general.meta.keywords" | t }}" />
  <meta name="author" content="{{ "general.meta.author" | t }}">
  {{ 'novafont.css.liquid'  | asset_url | stylesheet_tag }}
  <!-- Social meta ================================================== -->
  {% include 'social-meta-tags' %}

  <title>{{ page_title }}{% if current_tags %} &ndash; tagged "{{ current_tags | join: ', ' }}"{% endif %}{% if current_page != 1 %} &ndash; Page {{ current_page }}{% endif %}{% unless page_title contains shop.name %} &ndash; {{ shop.name }}{% endunless %}</title>
  {% if page_description %}<meta name="description" content="{{ page_description | escape }}" />{% endif %}
  <link rel="canonical" href="{{ canonical_url }}" />
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"></script>
  {{ 'jquery.colorbox.js' | asset_url | script_tag }}
  <script>
    $(function ()
    {
        $(".cboxiframe").colorbox({iframe:true, innerWidth:425, innerHeight:344});    
    })
</script>
  
  {% if settings.use_favicon %}<link rel="shortcut icon" href="{{ 'favicon.png' | asset_url }}" type="image/x-icon" />{% endif %}
  
  {% if settings.enable_custom_fonts %}
  	{{ 'fonts.js' | asset_url | script_tag }}
  {% else %}
  	{% include "header-fonts" %}
  {% endif %}
  
  {% include 'header-css' %}

  {{ 'responsive.css' | asset_url | stylesheet_tag }}

  {{ 'modernizr.js' | asset_url | script_tag }}
  
  {{ 'novafont.css.liquid' | asset_url | stylesheet_tag }}
  <!-- jQuery 1.10.1-->
  {{ 'jquery-2.1.4.min.js' | asset_url | script_tag }}
  {{ 'jquery.cookie.js' | asset_url | script_tag }}
  {% if template contains 'customers' %}
  {{ 'shopify_common.js' | shopify_asset_url | script_tag }}
  {{ 'customer_area.js'  | shopify_asset_url | script_tag }}
  {% endif %}
  
  {{ content_for_header }}

  

  <link href='https://fonts.googleapis.com/css?family=Carrois+Gothic' rel='stylesheet' type='text/css'>
  <link href='https://fonts.googleapis.com/css?family=Titillium+Web' rel='stylesheet' type='text/css'>
  <link href="//db.onlinewebfonts.com/c/efd2c83c0903c21ebe65d4fc8c2ef5bb?family=populairemedium" rel="stylesheet" type="text/css"/>

</head>

<body>

 {{ content_for_layout }}


</body>


</html>